        Scinet.public = (function () {
            function loadCartInfo(onLoad) {

                var formatCartItemCount = function (count) {
                        var format = (count > 1 || count === 0)
                                         ? "{0} items"
                                         : "{0} item";

                        $("#cartInfoTotalItemsCount").html(Scinet.stringFormat(format, count));
                    },
                    cookieCartItemCount = Cookies.get("Scinet5.CartItemsCount");

                if (onLoad && cookieCartItemCount != null) {
                    formatCartItemCount(parseInt(cookieCartItemCount, 10));
                } else {
                    $.ajax({ url: "/Cart/GetItemsCount", cache: false })
                        .done(
                            function (count) {
                                Cookies.set("Scinet5.CartItemsCount", count, { domain: ".scientific.net", secure: true });

                                formatCartItemCount(count);
                            });
                }
            }

            return {
                loadCartInfo: loadCartInfo
            };
        })();

        $(function () {
