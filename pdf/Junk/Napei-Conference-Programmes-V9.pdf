                    toggle.addEventListener('click', function(event) {
                        var parentLi = this.parentElement;
                        var submenu = parentLi.querySelector('.submenu');

                        if (submenu.classList.contains('active')) {
                            // Close this submenu
                            submenu.classList.remove('active');
                            // Optional: close all nested submenus as well
                            var submenus = submenu.querySelectorAll('.submenu.active');
                            submenus.forEach(function(sub) {
                                sub.classList.remove('active');
                            });
                        } else {
                            // Close all siblings' submenus
                            var openSubmenus = parentLi.parentNode.querySelectorAll('.submenu.active');
                            openSubmenus.forEach(function(openSubmenu) {
                                openSubmenu.classList.remove('active');
                                // Optional: close all nested submenus as well
                                var nestedSubmenus = openSubmenu.querySelectorAll('.submenu.active');
                                nestedSubmenus.forEach(function(nestedSub) {
                                    nestedSub.classList.remove('active');
                                });
                            });
                            // Open this submenu
                            submenu.classList.add('active');
                        }
                        event.stopPropagation();
                    });
                });

            });


        </script>
    </div>

