        <script>
            var lastTab;
            function searchRedirect() {
                var searchKey = encodeURIComponent($.trim($('#searchInput').val()));
                var searchType = $('#searchbar_type').val();

                switch(searchType){
                    case 'colleges': var section = 'where/'; break;
                    case 'courses': var section = 'what/'; break;
                    case 'scholarships': var section = 'scholarships/'; break;
                    case 'news': var section = 'education/news/'; break;
                    case 'events': var section = 'education/calendar.php'; break;
                    case 'articles': var section = 'education/search.php'; break;
                    default: break;
                }

                if(searchKey != '' && searchType != '' && searchKey.length > 0) {
                    window.location.href = '/' + section + "?keyword=" + $.trim(searchKey);
                    return false;
                }
                else {
                    return false;
                }
            }

            function openTab(tabLink, tabName) {
                const tabcontent = document.getElementsByClassName("search-tab-content");
                for (let i = 0; i < tabcontent.length; i++) {
                    tabcontent[i].style.display = "none";
                }
                const tablinks = document.getElementsByClassName("search-tab-link");
                for (let i = 0; i < tablinks.length; i++) {
                    tablinks[i].classList.remove("active");
                }
                document.getElementById(tabName).style.display = "block";
                tabLink.classList.add("active");
            }


            function debounce(func, delay) {
                let timeout;
                return function() {
                    const context = this;
                    const args = arguments;
                    clearTimeout(timeout);
                    timeout = setTimeout(() => {
                        func.apply(context, args);
                    }, delay);
                };
            }

            function handleSearch() {
                const query = this.value;
                const minQueryLength = 2;
                const resultsContainer = document.getElementById('searchResults');
                const newSearchWrap = document.getElementById('newSearchWrap');

                if (query.length <= minQueryLength) {
                    resetResults();
                    return;
                }

                const csrfToken = document.querySelector('meta[name="csrf-token"]').getAttribute('content');

                fetch(`/webway-api/search/all/?keyword=${encodeURIComponent(query)}`, {
                    method: 'GET',
                    headers: {
                        'Content-Type': 'application/json',
                        'CSRF-Token': csrfToken
                    }
                })
                    .then(response => response.json())
                    .then(data => {
                        resetResults();

                        let isEmpty = true;

                        if (data.ipts && Array.isArray(data.ipts)) {
                            appendResults('institution', data.ipts, query);
                            isEmpty = false;
                        }
                        if (data.courses && Array.isArray(data.courses)) {
                            appendResults('courses', data.courses, query);
                            isEmpty = false;
                        }
                        if (data.scholarships && Array.isArray(data.scholarships)) {
                            appendResults('scholarships', data.scholarships, query);
                            isEmpty = false;
                        }
                        if ((data.articles && Array.isArray(data.articles)) || data.news && Array.isArray(data.news)) {
                            let items = [...data.articles, ...data.news];
                            appendResults('articles', items, query);
                            isEmpty = false;
                        }

                        if (isEmpty) {
                            newSearchWrap.style.display = 'none';
                        } else {
                            newSearchWrap.style.display = 'block';
                            setActiveTab();
                        }
                    })
                    .catch(error => {
                        console.error('Error:', error);
                        resultsContainer.innerHTML = '<div>Error loading results</div>';
                    });
            }

            function resetResults() {
                lastTab = document.querySelector('.search-tab-link.active')?.id || 'searchtabinstitution';
                const tabMap = {
                    'searchtabinstitution': 'institutionResults',
                    'searchtabCourses': 'coursesResults',
                    'searchtabScholarships': 'scholarshipsResults',
                    'searchtabArticles': 'articlesResults'
                };

                document.getElementById('searchResults').innerHTML = `
        <div id="institutionResults" class="search-tab-content" style="display: block;"></div>
        <div id="coursesResults" class="search-tab-content"></div>
        <div id="scholarshipsResults" class="search-tab-content"></div>
        <div id="articlesResults" class="search-tab-content"></div>`;

                openTab(document.getElementById(lastTab), tabMap[lastTab]);
                document.getElementById(lastTab).classList.add('active');
                document.getElementById('newSearchWrap').style.display = 'none';
            }

            function appendResults(type, items, query) {
                const container = document.getElementById(`${type}Results`);
                const searchMoreLink = createSearchMoreLink(type, query);
                container.appendChild(searchMoreLink);

                items.forEach(item => {

                    let resultType = type;
                    if ('category' in item) {
                        resultType = 'news';
                    }

                    const itemDiv = createResultItem(resultType, item);
                    container.appendChild(itemDiv);
                });

                let count = items.length;

                if (count > 0) {
                    document.getElementById(`${type}Count`).innerHTML = `<span>`+count+`</span>`;
                } else {
                    document.getElementById(`${type}Count`).innerHTML = '';
                }

            }

            function createSearchMoreLink(type, query) {
                const div = document.createElement('div');
                const searchTypes = {
                    'institution': 'where',
                    'courses': 'what',
                    'scholarships': 'scholarships',
                    'articles': 'education/search.php'
                };

                let text = type;
                if (type === 'institution') {
                    text = 'institutions';
                }

                div.innerHTML = `<a class="searchMore" href="/${searchTypes[type]}?keyword=${encodeURIComponent(query)}">
        <i class="fa fa-search search-icon" aria-hidden="true"></i> Search all ${text} for <strong>${query}</strong>...</a>`;
                return div;
            }

            function createResultItem(type, item) {
                const div = document.createElement('div');
                let content = '';

                if (type === 'institution') {
                    content = `<a class="institutionLink" href="/where/profile.php?code=${item.code.toLowerCase()}">
            <img src="/images/college/logo/${item.logo}" class="search-logo" />
            <span>${item.name} <span class="text-sub2">Institution</span></span></a>`;
                } else if (type === 'courses') {
                    const award = item.award.length > 50 ? item.award.substring(0, 50) + '...' : item.award;
                    content = `<a class="institutionLink" href="/what/course/${item.code.toLowerCase()}/${item.id}">
            <img src="/images/college/logo/${item.logo}" class="search-logo" />
            <span>${award} <span class="text-sub2">${item.elevel}</span></span></a>`;
                } else if (type === 'scholarships') {
                    const fundname = item.fundname.length > 50 ? item.fundname.substring(0, 50) + '...' : item.fundname;
                    content = `<a class="institutionLink" href="/scholarships/${item.scholarship_slug}">
            <i class="fa fa-gift search-icon text-red" aria-hidden="true"></i>
            <span>${fundname} <span class="text-sub2">Scholarship</span></span></a>`;
                } else if (type === 'articles') {
                    const title = item.title.length > 50 ? item.title.substring(0, 50) + '...' : item.title;
                    content = `<a class="institutionLink" href="/education/${item.cat_slug}/${item.slug}">
            <img src="/images/education/${item.thumb_image}" class="search-logo search-img" />
            <span>${title} <span class="text-sub2">Article</span></span></a>`;
                } else if (type === 'news') {
                    const title = item.title.length > 50 ? item.title.substring(0, 50) + '...' : item.title;
                    let img = '<i class="fa fa-newspaper-o search-icon" aria-hidden="true"></i>';
                    if (item.news_thumbnail && item.news_thumbnail !== '') {
                        item.news_thumbnail = img;
                    }

                    content = `<a class="institutionLink" href="/education/news/${item.date}">
            ${img}
            <span>${title} <span class="text-sub2">News</span></span></a>`;
                }

                div.innerHTML = content;
                return div;
            }

            function setActiveTab() {
                let activeTab = document.querySelector('.search-tab-link.active')?.id || 'searchtabinstitution';
                const tabMap = {
                    'searchtabinstitution': 'institutionResults',
                    'searchtabCourses': 'coursesResults',
                    'searchtabScholarships': 'scholarshipsResults',
                    'searchtabArticles': 'articlesResults'
                };

                openTab(document.getElementById(activeTab), tabMap[activeTab]);
                document.getElementById(activeTab).classList.add('active');
            }


            document.getElementById('searchInput').addEventListener('focus', debounce(handleSearch, 300));
            document.getElementById('searchInput').addEventListener('input', debounce(handleSearch, 300));
            document.addEventListener('click', function(event) {
                var isClickInsideSearchWrap = document.getElementById('search_wrap').contains(event.target);

                if (!isClickInsideSearchWrap) {
                    document.getElementById('newSearchWrap').style.display = 'none';
                    openTab(document.getElementById('searchtabinstitution'), 'institutionResults');
                }
            });
            document.addEventListener("DOMContentLoaded", function() {
                document.querySelector('.search-tab-link').click(); // Click on the first tab link to open it by default
            });
        </script>
    </div>
</div>


